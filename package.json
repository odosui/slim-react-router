{
  "name": "slim-react-router",
  "version": "0.6.0",
  "description": "A lightweight alternative to React Router with familiar API - BrowserRouter, HashRouter, Route, Switch, Link, and hooks",
  "author": "",
  "homepage": "",
  "repository": "",
  "license": "MIT",
  "keywords": [
    "react",
    "router",
    "routing",
    "browser-router",
    "hash-router",
    "navigation",
    "lightweight",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "start": "vite --host --open",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "test:watch": "vitest",
    "test": "vitest run"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "jsdom": "^27.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.14"
  }
}
